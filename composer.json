{
    "name": "word-on-fire/wof-core",
    "description": "A collection of libraries, tools, and shared functionality across WOF WordPress sites",
    "version": "1.0.8",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sean Davis",
            "email": "sean.davis@wordonfire.org"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "WOF\\": "src",
            "WOF\\Org\\Theme\\": "wordpress/wp-content/themes/wof-theme/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WOF\\Tests\\": "test/php-tests/",
            "WOF\\Mocks\\": "test/mocks"
        }
    },
    "require": {
        "php": "^7.4",
        "algolia/algoliasearch-client-php": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    }
}
